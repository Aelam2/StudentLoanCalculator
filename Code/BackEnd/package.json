{
  "name": "student-loan-calculator-back-end",
  "version": "1.0.0",
  "description": "Rest API built on ExpressJS built to service the Student Loan Calculator Front-End",
  "main": "index.js",
  "scripts": {
    "postinstall": "patch-package",
    "watch": "nodemon --watch lib --exec npm run start",
    "start": "npm run build && npm run serve",
    "build": "babel ./lib -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --require @babel/register",
    "db:migrate": "npm run build && npx sequelize db:migrate"
  },
  "author": "Alex Elam",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "patch-package": "^6.2.1",
    "sequelize-cli": "^5.5.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.21.5",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "tedious": "^8.2.0"
  }
}
